import cv2
import mediapipe as mp
mp_drawing = mp.solutions.drawing_utils
mp_drawing_styles = mp.solutions.drawing_styles
mp_hands = mp.solutions.hands
cap = cv2.VideoCapture(0)
with mp_hands.Hands(
    model_complexity=0,
    min_detection_confidence=0.5,
    min_tracking_confidence=0.5) as hands:
  while cap.isOpened():
    success, image = cap.read()
    if not success:
      print("Ignoring empty camera frame.")
    image.flags.writeable = False
    image = cv2.cvtColor(image, cv2.COLOR_BGR2RGB)
    results = hands.process(image)
    image.flags.writeable = True
    image = cv2.cvtColor(image, cv2.COLOR_RGB2BGR)
    if results.multi_hand_landmarks:
      for hand_landmarks in results.multi_hand_landmarks:
        mp_drawing.draw_landmarks(
            image,
            hand_landmarks,
            mp_hands.HAND_CONNECTIONS,
            mp_drawing_styles.get_default_hand_landmarks_style(),
            mp_drawing_styles.get_default_hand_connections_style())
    if results.multi_hand_landmarks:
        if results.multi_hand_landmarks[0].landmark[8].y>results.multi_hand_landmarks[0].landmark[7].y and results.multi_hand_landmarks[0].landmark[7].y>results.multi_hand_landmarks[0].landmark[6].y and results.multi_hand_landmarks[0].landmark[12].y>results.multi_hand_landmarks[0].landmark[11].y and results.multi_hand_landmarks[0].landmark[11].y>results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[16].y>results.multi_hand_landmarks[0].landmark[15].y and results.multi_hand_landmarks[0].landmark[8].y>results.multi_hand_landmarks[0].landmark[5].y and results.multi_hand_landmarks[0].landmark[12].y>results.multi_hand_landmarks[0].landmark[9].y and results.multi_hand_landmarks[0].landmark[4].y<results.multi_hand_landmarks[0].landmark[3].y and results.multi_hand_landmarks[0].landmark[20].x<results.multi_hand_landmarks[0].landmark[4].x :
            print("A")

        if results.multi_hand_landmarks[0].landmark[5].x < results.multi_hand_landmarks[0].landmark[9].x and results.multi_hand_landmarks[0].landmark[9].x < results.multi_hand_landmarks[0].landmark[13].x and results.multi_hand_landmarks[0].landmark[13].x < results.multi_hand_landmarks[0].landmark[17].x and results.multi_hand_landmarks[0].landmark[9].y > results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[10].y > results.multi_hand_landmarks[0].landmark[11].y and results.multi_hand_landmarks[0].landmark[11].y > results.multi_hand_landmarks[0].landmark[12].y and results.multi_hand_landmarks[0].landmark[9].y > results.multi_hand_landmarks[0].landmark[12].y and results.multi_hand_landmarks[0].landmark[8].x > results.multi_hand_landmarks[0].landmark[4].x:
            print("B")

        if results.multi_hand_landmarks[0].landmark[7].y > results.multi_hand_landmarks[0].landmark[6].y and results.multi_hand_landmarks[0].landmark[7].y > results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[7].x > results.multi_hand_landmarks[0].landmark[10].x and results.multi_hand_landmarks[0].landmark[2].y < results.multi_hand_landmarks[0].landmark[9].y and results.multi_hand_landmarks[0].landmark[2].y < results.multi_hand_landmarks[0].landmark[13].y and results.multi_hand_landmarks[0].landmark[2].y < results.multi_hand_landmarks[0].landmark[17].y and results.multi_hand_landmarks[0].landmark[4].y < results.multi_hand_landmarks[0].landmark[0].y and results.multi_hand_landmarks[0].landmark[5].y > results.multi_hand_landmarks[0].landmark[2].y:
            print("Г")
        if results.multi_hand_landmarks[0].landmark[4].y > results.multi_hand_landmarks[0].landmark[8].y and results.multi_hand_landmarks[0].landmark[4].y > results.multi_hand_landmarks[0].landmark[12].y and results.multi_hand_landmarks[0].landmark[4].x > results.multi_hand_landmarks[0].landmark[5].x and results.multi_hand_landmarks[0].landmark[8].y > results.multi_hand_landmarks[0].landmark[6].y and results.multi_hand_landmarks[0].landmark[12].y > results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[16].y > results.multi_hand_landmarks[0].landmark[14].y and results.multi_hand_landmarks[0].landmark[4].y < results.multi_hand_landmarks[0].landmark[0].y and results.multi_hand_landmarks[0].landmark[4].y < results.multi_hand_landmarks[0].landmark[5].y and results.multi_hand_landmarks[0].landmark[8].y > results.multi_hand_landmarks[0].landmark[6].y and results.multi_hand_landmarks[0].landmark[18].y < results.multi_hand_landmarks[0].landmark[20].y:
            print("E")

        if results.multi_hand_landmarks[0].landmark[4].y < results.multi_hand_landmarks[0].landmark[1].y and results.multi_hand_landmarks[0].landmark[8].x > results.multi_hand_landmarks[0].landmark[7].x and results.multi_hand_landmarks[0].landmark[7].x > results.multi_hand_landmarks[0].landmark[6].x and results.multi_hand_landmarks[0].landmark[6].x > results.multi_hand_landmarks[0].landmark[5].x and results.multi_hand_landmarks[0].landmark[20].x > results.multi_hand_landmarks[0].landmark[19].x and results.multi_hand_landmarks[0].landmark[19].x > results.multi_hand_landmarks[0].landmark[18].x and results.multi_hand_landmarks[0].landmark[18].x > results.multi_hand_landmarks[0].landmark[17].x and results.multi_hand_landmarks[0].landmark[1].y > results.multi_hand_landmarks[0].landmark[17].y and results.multi_hand_landmarks[0].landmark[4].y > results.multi_hand_landmarks[0].landmark[5].y and results.multi_hand_landmarks[0].landmark[4].x < results.multi_hand_landmarks[0].landmark[8].x:
            print("Ж")
        if results.multi_hand_landmarks[0].landmark[16].y < results.multi_hand_landmarks[0].landmark[15].y and results.multi_hand_landmarks[0].landmark[15].y < results.multi_hand_landmarks[0].landmark[14].y and results.multi_hand_landmarks[0].landmark[20].y < results.multi_hand_landmarks[0].landmark[19].y and results.multi_hand_landmarks[0].landmark[19].y < results.multi_hand_landmarks[0].landmark[18].y and  results.multi_hand_landmarks[0].landmark[8].y > results.multi_hand_landmarks[0].landmark[4].y and results.multi_hand_landmarks[0].landmark[5].y < results.multi_hand_landmarks[0].landmark[2].y and results.multi_hand_landmarks[0].landmark[4].x < results.multi_hand_landmarks[0].landmark[20].x:
            print("И")
        if results.multi_hand_landmarks[0].landmark[6].y < results.multi_hand_landmarks[0].landmark[8].y and results.multi_hand_landmarks[0].landmark[12].y > results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[16].y < results.multi_hand_landmarks[0].landmark[14].y and results.multi_hand_landmarks[0].landmark[20].y < results.multi_hand_landmarks[0].landmark[18].y and results.multi_hand_landmarks[0].landmark[4].y > results.multi_hand_landmarks[0].landmark[0].y and results.multi_hand_landmarks[0].landmark[4].x > results.multi_hand_landmarks[0].landmark[12].x:
            print("Л")
        if results.multi_hand_landmarks[0].landmark[6].y < results.multi_hand_landmarks[0].landmark[8].y and results.multi_hand_landmarks[0].landmark[12].y > results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[16].y > results.multi_hand_landmarks[0].landmark[14].y and results.multi_hand_landmarks[0].landmark[20].y < results.multi_hand_landmarks[0].landmark[15].y and results.multi_hand_landmarks[0].landmark[4].y > results.multi_hand_landmarks[0].landmark[0].y and results.multi_hand_landmarks[0].landmark[4].x < results.multi_hand_landmarks[0].landmark[8].x:
            print("M")
        if results.multi_hand_landmarks[0].landmark[16].y > results.multi_hand_landmarks[0].landmark[14].y and results.multi_hand_landmarks[0].landmark[5].y > results.multi_hand_landmarks[0].landmark[8].y and results.multi_hand_landmarks[0].landmark[9].y > results.multi_hand_landmarks[0].landmark[12].y and results.multi_hand_landmarks[0].landmark[17].y > results.multi_hand_landmarks[0].landmark[20].y and results.multi_hand_landmarks[0].landmark[8].y < results.multi_hand_landmarks[0].landmark[6].y and results.multi_hand_landmarks[0].landmark[20].y<results.multi_hand_landmarks[0].landmark[18].y:
            print("H")
        if results.multi_hand_landmarks[0].landmark[4].y < results.multi_hand_landmarks[0].landmark[8].y  and results.multi_hand_landmarks[0].landmark[4].x > results.multi_hand_landmarks[0].landmark[5].x and results.multi_hand_landmarks[0].landmark[8].y > results.multi_hand_landmarks[0].landmark[6].y  and results.multi_hand_landmarks[0].landmark[0].y > results.multi_hand_landmarks[0].landmark[8].y and results.multi_hand_landmarks[0].landmark[4].y < results.multi_hand_landmarks[0].landmark[0].y and results.multi_hand_landmarks[0].landmark[4].y < results.multi_hand_landmarks[0].landmark[5].y and results.multi_hand_landmarks[0].landmark[8].y > results.multi_hand_landmarks[0].landmark[6].y and results.multi_hand_landmarks[0].landmark[18].y > results.multi_hand_landmarks[0].landmark[20].y and results.multi_hand_landmarks[0].landmark[4].x > results.multi_hand_landmarks[0].landmark[20].x :
            print("О")
        if results.multi_hand_landmarks[0].landmark[6].y < results.multi_hand_landmarks[0].landmark[8].y and results.multi_hand_landmarks[0].landmark[12].y > results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[16].y < results.multi_hand_landmarks[0].landmark[14].y and results.multi_hand_landmarks[0].landmark[20].y < results.multi_hand_landmarks[0].landmark[18].y and results.multi_hand_landmarks[0].landmark[4].y > results.multi_hand_landmarks[0].landmark[0].y and results.multi_hand_landmarks[0].landmark[4].x < results.multi_hand_landmarks[0].landmark[12].x:
            print("П")
        if results.multi_hand_landmarks[0].landmark[12].y > results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[5].y > results.multi_hand_landmarks[0].landmark[8].y and results.multi_hand_landmarks[0].landmark[13].y > results.multi_hand_landmarks[0].landmark[16].y and results.multi_hand_landmarks[0].landmark[17].y > results.multi_hand_landmarks[0].landmark[20].y and results.multi_hand_landmarks[0].landmark[8].y < results.multi_hand_landmarks[0].landmark[6].y:
            print("Р")
        if results.multi_hand_landmarks[0].landmark[4].y < results.multi_hand_landmarks[0].landmark[1].y and results.multi_hand_landmarks[0].landmark[8].x > results.multi_hand_landmarks[0].landmark[7].x and results.multi_hand_landmarks[0].landmark[7].x > results.multi_hand_landmarks[0].landmark[6].x and results.multi_hand_landmarks[0].landmark[6].x > results.multi_hand_landmarks[0].landmark[5].x and results.multi_hand_landmarks[0].landmark[20].x > results.multi_hand_landmarks[0].landmark[19].x and results.multi_hand_landmarks[0].landmark[19].x > results.multi_hand_landmarks[0].landmark[18].x and results.multi_hand_landmarks[0].landmark[18].x > results.multi_hand_landmarks[0].landmark[17].x and results.multi_hand_landmarks[0].landmark[1].y > results.multi_hand_landmarks[0].landmark[17].y and results.multi_hand_landmarks[0].landmark[4].y > results.multi_hand_landmarks[0].landmark[5].y and results.multi_hand_landmarks[0].landmark[4].x > results.multi_hand_landmarks[0].landmark[8].x:
            print("С")
        if results.multi_hand_landmarks[0].landmark[6].y < results.multi_hand_landmarks[0].landmark[8].y and results.multi_hand_landmarks[0].landmark[12].y > results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[16].y > results.multi_hand_landmarks[0].landmark[14].y and results.multi_hand_landmarks[0].landmark[20].y < results.multi_hand_landmarks[0].landmark[15].y and results.multi_hand_landmarks[0].landmark[4].y > results.multi_hand_landmarks[0].landmark[0].y and results.multi_hand_landmarks[0].landmark[4].x > results.multi_hand_landmarks[0].landmark[8].x:
            print("T")
        if results.multi_hand_landmarks[0].landmark[8].y>results.multi_hand_landmarks[0].landmark[7].y and results.multi_hand_landmarks[0].landmark[7].y>results.multi_hand_landmarks[0].landmark[6].y and results.multi_hand_landmarks[0].landmark[12].y>results.multi_hand_landmarks[0].landmark[11].y and results.multi_hand_landmarks[0].landmark[11].y>results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[16].y>results.multi_hand_landmarks[0].landmark[15].y and results.multi_hand_landmarks[0].landmark[8].y>results.multi_hand_landmarks[0].landmark[5].y and results.multi_hand_landmarks[0].landmark[12].y>results.multi_hand_landmarks[0].landmark[9].y and results.multi_hand_landmarks[0].landmark[4].y<results.multi_hand_landmarks[0].landmark[3].y and results.multi_hand_landmarks[0].landmark[18].y>results.multi_hand_landmarks[0].landmark[20].y:
            print("У")
        if results.multi_hand_landmarks[0].landmark[8].y>results.multi_hand_landmarks[0].landmark[7].y and results.multi_hand_landmarks[0].landmark[7].y>results.multi_hand_landmarks[0].landmark[6].y and results.multi_hand_landmarks[0].landmark[12].y>results.multi_hand_landmarks[0].landmark[11].y and results.multi_hand_landmarks[0].landmark[11].y>results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[16].y>results.multi_hand_landmarks[0].landmark[15].y and results.multi_hand_landmarks[0].landmark[8].y>results.multi_hand_landmarks[0].landmark[5].y and results.multi_hand_landmarks[0].landmark[12].y>results.multi_hand_landmarks[0].landmark[9].y and results.multi_hand_landmarks[0].landmark[4].y<results.multi_hand_landmarks[0].landmark[3].y and results.multi_hand_landmarks[0].landmark[20].y>results.multi_hand_landmarks[0].landmark[4].y and results.multi_hand_landmarks[0].landmark[8].y>results.multi_hand_landmarks[0].landmark[20].y and results.multi_hand_landmarks[0].landmark[20].x>results.multi_hand_landmarks[0].landmark[4].x:
            print("Ф")
        if results.multi_hand_landmarks[0].landmark[20].y > results.multi_hand_landmarks[0].landmark[4].y and results.multi_hand_landmarks[0].landmark[12].y > results.multi_hand_landmarks[0].landmark[8].y and results.multi_hand_landmarks[0].landmark[20].x > results.multi_hand_landmarks[0].landmark[4].x and results.multi_hand_landmarks[0].landmark[8].y < results.multi_hand_landmarks[0].landmark[6].y:
            print("Х")
        if results.multi_hand_landmarks[0].landmark[8].y>results.multi_hand_landmarks[0].landmark[7].y and results.multi_hand_landmarks[0].landmark[7].y>results.multi_hand_landmarks[0].landmark[6].y and results.multi_hand_landmarks[0].landmark[12].y>results.multi_hand_landmarks[0].landmark[11].y and results.multi_hand_landmarks[0].landmark[11].y>results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[16].y>results.multi_hand_landmarks[0].landmark[15].y  and results.multi_hand_landmarks[0].landmark[12].y>results.multi_hand_landmarks[0].landmark[9].y and results.multi_hand_landmarks[0].landmark[4].y<results.multi_hand_landmarks[0].landmark[3].y and results.multi_hand_landmarks[0].landmark[4].y>results.multi_hand_landmarks[0].landmark[7].y:
            print("Ч")
        if results.multi_hand_landmarks[0].landmark[5].x < results.multi_hand_landmarks[0].landmark[9].x and results.multi_hand_landmarks[0].landmark[9].x < results.multi_hand_landmarks[0].landmark[13].x and results.multi_hand_landmarks[0].landmark[13].x < results.multi_hand_landmarks[0].landmark[17].x and results.multi_hand_landmarks[0].landmark[9].y > results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[10].y > results.multi_hand_landmarks[0].landmark[11].y and results.multi_hand_landmarks[0].landmark[11].y > results.multi_hand_landmarks[0].landmark[12].y and results.multi_hand_landmarks[0].landmark[9].y > results.multi_hand_landmarks[0].landmark[12].y and results.multi_hand_landmarks[0].landmark[8].x < results.multi_hand_landmarks[0].landmark[4].x and results.multi_hand_landmarks[0].landmark[20].y > results.multi_hand_landmarks[0].landmark[14].y:
            print("Ш")
        if results.multi_hand_landmarks[0].landmark[8].y<results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[7].y<results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[12].y>results.multi_hand_landmarks[0].landmark[11].y and  results.multi_hand_landmarks[0].landmark[16].y>results.multi_hand_landmarks[0].landmark[15].y  and results.multi_hand_landmarks[0].landmark[12].y>results.multi_hand_landmarks[0].landmark[9].y and results.multi_hand_landmarks[0].landmark[4].y<results.multi_hand_landmarks[0].landmark[3].y and results.multi_hand_landmarks[0].landmark[4].y>results.multi_hand_landmarks[0].landmark[7].y:
            print("Э")
        if results.multi_hand_landmarks[0].landmark[4].y > results.multi_hand_landmarks[0].landmark[8].y and results.multi_hand_landmarks[0].landmark[4].y > results.multi_hand_landmarks[0].landmark[12].y and results.multi_hand_landmarks[0].landmark[4].x > results.multi_hand_landmarks[0].landmark[5].x and results.multi_hand_landmarks[0].landmark[8].y > results.multi_hand_landmarks[0].landmark[6].y and results.multi_hand_landmarks[0].landmark[12].y > results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[16].y > results.multi_hand_landmarks[0].landmark[14].y and results.multi_hand_landmarks[0].landmark[4].y < results.multi_hand_landmarks[0].landmark[0].y and results.multi_hand_landmarks[0].landmark[4].y < results.multi_hand_landmarks[0].landmark[5].y and results.multi_hand_landmarks[0].landmark[8].y > results.multi_hand_landmarks[0].landmark[6].y and results.multi_hand_landmarks[0].landmark[18].y > results.multi_hand_landmarks[0].landmark[20].y:
            print("Ю")
        if results.multi_hand_landmarks[0].landmark[6].y>results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[7].y<results.multi_hand_landmarks[0].landmark[10].y and results.multi_hand_landmarks[0].landmark[12].y<results.multi_hand_landmarks[0].landmark[11].y and  results.multi_hand_landmarks[0].landmark[16].y>results.multi_hand_landmarks[0].landmark[15].y  and results.multi_hand_landmarks[0].landmark[4].y<results.multi_hand_landmarks[0].landmark[3].y and results.multi_hand_landmarks[0].landmark[4].y>results.multi_hand_landmarks[0].landmark[7].y and results.multi_hand_landmarks[0].landmark[20].y>results.multi_hand_landmarks[0].landmark[18].y:
            print("Я")
    cv2.imshow('MediaPipe Hands', cv2.flip(image, 1))
    if cv2.waitKey(5) & 0xFF == 27:
